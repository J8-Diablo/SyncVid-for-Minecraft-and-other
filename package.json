{
  "name": "syncvid-for-minecraft-and-other",
  "version": "1.0.3",
  "main": "server.js",
  "bin": "server.js",
  "scripts": {
    "start": "node server.js",
    "build": "nexe server.js --build --output dist/syncvid.exe",
    "postbuild": "copyfiles -u 1 \"public/**/*\" \"locales/**/*\" dist/"
  },
  "dependencies": {
    "express": "^4.17.1",
    "http": "^0.0.1-security",
    "socket.io": "^4.0.0",
    "multer": "^1.4.3",
    "video.js": "^8.5.2",
    "i18next": "^23.x",
    "i18next-browser-languagedetector": "^6.x",
    "serve-static": "^1.x"
  },
  
  "devDependencies": {
    "copyfiles": "^2.4.1"
  },
  "pkg": {
    "targets": [
      "node18-win-x64"
    ],
    "assets": [
      "public/**/*",
      "locales/**/*"
    ],
    "executable": true,
    "output": "dist/syncvid"
  }
}
